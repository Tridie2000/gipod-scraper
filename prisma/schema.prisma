generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./gipod.sqlite"
}

model Closure {
  id          String   @id
  start       DateTime
  end         DateTime
  lastUpdate  DateTime
  description String?
  hindrance   String
  geometry    String
  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt

  @@map("closure")
}
